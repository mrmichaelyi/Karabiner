{
  "title": "Multitouch Shortcuts",
  "rules": [
    {
      "description": "Multitouch wsad: w->UpPage, s->DownPage, a->left, b->right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a"
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d"
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w"
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s"
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        }
      ]
    },
