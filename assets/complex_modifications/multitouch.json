{
  "title": "Multitouch Shortcuts",
  "rules": [
    {
      "description": "Multitouch wsad: w->UpPage, s->DownPage, a->left, b->right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a"
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "multitouch_extension_finger_count_total",
              "value": 1
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d"
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "w"
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s"
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        }
      ]
    },
    {
      "description": "Multitouch 1/2: 1->Ctrl+Shift+Tab, 3->Command+Click",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1"
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control",
                "shift"
              ]
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "2"
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control"
              ]
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        }
      ]
    },
    {
      "description": "Multitouch 3: 3->Command+Click",
      "manipulators": [
        {
          "type": "basic",
          "description": "3->Command+Click",
          "from": {
            "key_code": "3"
          },
          "to": [
            {
              "pointing_button": "button1",
              "modifiers": [
                "command"
              ]
            }
          ],
          "conditions": [
            {
              "type": "variable_unless",
              "name": "multitouch_extension_finger_count_total",
              "value": 0
            }
          ]
        }
      ]
    }
  ]
}
